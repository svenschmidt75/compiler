nterm1	->	nterm1	nterm2	'TERM'
		|	'EPSILON'

nterm2	->	'EPSILON'

-----------------------------------

FIRST(nterm1) =
{
	TERM
}


***************************************************************************
***************************************************************************

nterm1	->	nterm2	nterm3	'TERM1'

nterm2	->	'TERM2'

nterm3	->	'TERM3'

-----------------------------------

FIRST( nterm1 ) =
{
	TERM2
}

***************************************************************************
***************************************************************************


nterm1	->	nterm2	nterm3	'TERM1'

nterm2	->	'EPSILON'
		|	'TERM2'

nterm3	->	'TERM3'

-----------------------------------

FIRST( nterm1 ) =
{
	TERM2, TERM3
}



***************************************************************************
***************************************************************************


nterm1	->	nterm2	nterm3	'TERM1'

nterm2	->	'EPSILON'
		|	'TERM2'

nterm3	->	'TERM3'

-----------------------------------

FIRST( nterm2 ) =
{
	EPSILON, TERM2
}


***************************************************************************
***************************************************************************


nterm1	->	nterm2	nterm3	'TERM1'

nterm2	->	'EPSILON'
		|	'TERM2'

nterm3	->	'TERM3'
		|	'EPSILON'

-----------------------------------

FIRST( nterm1 ) =
{
	TERM2, TERM3, TERM1
}


***************************************************************************
***************************************************************************


nterm1	->	nterm2	nterm3

nterm2	->	'EPSILON'
		|	'TERM2'

nterm3	->	'TERM3'
		|	'EPSILON'

-----------------------------------

FIRST( nterm1 ) =
{
	TERM2, TERM3, EPSILON
}


***************************************************************************
***************************************************************************


nterm1	->	nterm2	nterm3 'TERM1'

nterm2	->	'EPSILON'
		|	nterm2 'TERM2'

nterm3	->	'TERM3'

-----------------------------------

FIRST( nterm1 ) =
{
	TERM2, TERM3
}


***************************************************************************
***************************************************************************


nterm1	->	nterm2	nterm3 'TERM1'

nterm2	->	nterm3 nterm4
		|	nterm2 'TERM2'

nterm3	->	'TERM3'
		|	'EPSILON'

nterm4	->	'EPSILON'

-----------------------------------

FIRST( nterm1 ) =
{
	TERM1, TERM2, TERM3
}

FIRST( nterm2 ) =
{
	TERM3, EPSILON, TERM2
}


